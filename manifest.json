{
    "manifest_version": 2,
    "name": "Editor",
    "version": "1.0",
    "description": "文字校正排版",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["activeTab", "storage", "declarativeContent"],
    "options_page": "options.html",
    "page_action": {
        "default_icon": "icon.png"
    }
}
